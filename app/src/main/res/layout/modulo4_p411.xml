        android:layout_height="wrap_content"
        android:theme="@style/TemaTextoPregunta"
        android:text="@string/modulo_4_p411"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/modulo_4_p411_ind"/>
    <CheckBox
        android:id="@+id/mod4_411_checkbox_C4_P411_1"
