<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:layout_margin="@dimen/margen_layout_pregunta"
    android:padding="@dimen/padding_layout_pregunta"
    android:background="@drawable/marco_pregunta">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/TemaTextoPregunta"
        android:text="@string/modulo_5_p512"/>
    <RadioGroup
        android:id="@+id/mod5_512_radiogroup_C5_P512"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone"/>
        <RadioButton
            android:id="@+id/mod5_512_rb1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/modulo_5_p512_sp1"/>
        <RadioButton
            android:id="@+id/mod5_512_rb2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/modulo_5_p512_sp2"/>
        <RadioButton
            android:id="@+id/mod5_512_rb3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/modulo_5_p512_sp3"/>
        <RadioButton
            android:id="@+id/mod5_512_rb4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/modulo_5_p512_sp4"/>
        <RadioButton
            android:id="@+id/mod5_512_rb5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/modulo_5_p512_sp5"/>
        <RadioButton
            android:id="@+id/mod5_512_rb6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/margen_small"
            android:text="@string/modulo_5_p512_sp6"/>
        <EditText
            android:id="@+id/mod5_512_edittext_C5_P512_O"
            style="@style/TemaInputEspecifiqueRadioIntermedio"/>
        <RadioButton
            android:id="@+id/mod5_512_rb7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/modulo_5_p512_sp7"/>
    </RadioGroup>
</LinearLayout>