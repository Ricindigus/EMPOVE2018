    <CheckBox
        android:id="@+id/mod4_411_checkbox_C4_P411_6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/modulo_4_p411_sp6"/>
    <CheckBox
        android:id="@+id/mod4_411_checkbox_C4_P411_7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
